&i2c0 {
	vdd_cpu_big0_s0: vdd_cpu_big0_mem_s0: rk8602@42 {
		compatible = "rockchip,rk8602";
		reg = <0x42>;
		vin-supply = <&vcc5v0_sys>;
		regulator-compatible = "rk860x-reg";
		regulator-name = "vdd_cpu_big0_s0";
		regulator-min-microvolt = <550000>;
		regulator-max-microvolt = <1050000>;
		regulator-ramp-delay = <2300>;
		rockchip,suspend-voltage-selector = <1>;
		regulator-boot-on;
		regulator-always-on;
		regulator-state-mem {
			regulator-off-in-suspend;
		};
	};

	vdd_cpu_big1_s0: vdd_cpu_big1_mem_s0: rk8603@43 {
		compatible = "rockchip,rk8603";
		reg = <0x43>;
		vin-supply = <&vcc5v0_sys>;
		regulator-compatible = "rk860x-reg";
		regulator-name = "vdd_cpu_big1_s0";
		regulator-min-microvolt = <550000>;
		regulator-max-microvolt = <1050000>;
		regulator-ramp-delay = <2300>;
		rockchip,suspend-voltage-selector = <1>;
		regulator-boot-on;
		regulator-always-on;
		regulator-state-mem {
			regulator-off-in-suspend;
		};
	};
};

&i2c1 {
	vdd_npu_s0: vdd_npu_mem_s0: rk8602@42 {
		compatible = "rockchip,rk8602";
		reg = <0x42>;
		vin-supply = <&vcc5v0_sys>;
		regulator-compatible = "rk860x-reg";
		regulator-name = "vdd_npu_s0";
		regulator-min-microvolt = <550000>;
		regulator-max-microvolt = <950000>;
		regulator-ramp-delay = <2300>;
		rockchip,suspend-voltage-selector = <1>;
		regulator-boot-on;
		regulator-always-on;
		regulator-state-mem {
			regulator-off-in-suspend;
		};
	};
};

&i2c2 {
	aw9523: gpio-expander@5b {
		compatible = "awinic,aw9523-pinctrl";
		reg = <0x5b>;
		gpio-controller;
		#gpio-cells = <2>;

		pinctrl-names = "default";
		pinctrl-0 = <&aw9523_int>;

		interrupts-extended = <&gpio1 RK_PA4 IRQ_TYPE_EDGE_FALLING>;

		status = "okay";

		/* GPIO_P00 */
		/omit-if-no-ref/
		aw9523_cam0_reset_pins: aw9523-cam0-reset-pins {
			pins = "gpio0";
			function = "gpio";
			input-disable;
			output-high;
		};

		/* GPIO_P01 */
		/omit-if-no-ref/
		aw9523_cam0_powerdown_pins: aw9523-cam0-powerdown-pins {
			pins = "gpio1";
			function = "gpio";
			input-disable;
			output-low;
		};

		/* GPIO_P02 */
		/omit-if-no-ref/
		aw9523_cam1_reset_pins: aw9523-cam1-reset-pins {
			pins = "gpio2";
			function = "gpio";
			input-disable;
			output-high;
		};

		/* GPIO_P03 */
		/omit-if-no-ref/
		aw9523_cam1_powerdown_pins: aw9523-cam1-powerdown-pins {
			pins = "gpio3";
			function = "gpio";
			input-disable;
			output-low;
		};

		/* GPIO_P04 */
		/omit-if-no-ref/
		aw9523_cam2_reset_pins: aw9523-cam2-reset-pins {
			pins = "gpio4";
			function = "gpio";
			input-disable;
			output-high;
		};

		/* GPIO_P05 */
		/omit-if-no-ref/
		aw9523_cam2_powerdown_pins: aw9523-cam2-powerdown-pins {
			pins = "gpio5";
			function = "gpio";
			input-disable;
			output-low;
		};

		/* GPIO_P06 */
		/omit-if-no-ref/
		aw9523_cam3_reset_pins: aw9523-cam3-reset-pins {
			pins = "gpio6";
			function = "gpio";
			input-disable;
			output-high;
		};

		/* GPIO_P07 */
		/omit-if-no-ref/
		aw9523_cam3_powerdown_pins: aw9523-cam3-powerdown-pins {
			pins = "gpio7";
			function = "gpio";
			input-disable;
			output-low;
		};

		/* GPIO_P10 */
		/omit-if-no-ref/
		aw9523_cam4_reset_pins: aw9523-cam4-reset-pins {
			pins = "gpio8";
			function = "gpio";
			input-disable;
			output-high;
		};

		/* GPIO_P11 */
		/omit-if-no-ref/
		aw9523_cam4_powerdown_pins: aw9523-cam4-powerdown-pins {
			pins = "gpio9";
			function = "gpio";
			input-disable;
			output-low;
		};

		/* GPIO_P12 */
		/omit-if-no-ref/
		aw9523_cam5_reset_pins: aw9523-cam5-reset-pins {
			pins = "gpio10";
			function = "gpio";
			input-disable;
			output-high;
		};

		/* GPIO_P13 */
		/omit-if-no-ref/
		aw9523_cam5_powerdown_pins: aw9523-cam5-powerdown-pins {
			pins = "gpio11";
			function = "gpio";
			input-disable;
			output-low;
		};

		/* GPIO_P14 */
		/omit-if-no-ref/
		aw9523_uart_rs485_re_de_pins: aw9523-uart-rs485-re-de-pins {
			pins = "gpio12";
			function = "gpio";
			input-disable;
			output-high;
		};

		/* GPIO_P15 */
		/omit-if-no-ref/
		aw9523_bt_host_wake_pins: aw9523-bt-host-wake-pins {
			pins = "gpio13";
			function = "gpio";
			input-disable;
			output-low;
		};

		/* GPIO_P16 */
		/omit-if-no-ref/
		aw9523_wifi_disable_pins: aw9523-wifi-disable-pins {
			pins = "gpio14";
			function = "gpio";
			input-disable;
			output-high;
		};

		/* GPIO_P17 */
		/omit-if-no-ref/
		aw9523_bt_disable_pins: aw9523-bt-disable-pins {
			pins = "gpio15";
			function = "gpio";
			input-disable;
			output-high;
		};

		/* GPIO_P16 / GPIO_P17 */
		/omit-if-no-ref/
		aw9523_led_pins: aw9523-led-pins {
			pins = "gpio14", "gpio15";
			function = "gpio";
			input-disable;
			output-low;
		};
	};
};

